{
    "name": "jaroslavtyc/doctrineum-generic",
    "type": "library",
    "description": "Enumeration type for Doctrine",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Doctrineum\\": "Doctrineum/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jaroslavtyc/granam-strict-object"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jaroslavtyc/granam-exception-hierarchy"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/dbal": "2.4.*",
        "granam/strict-object": "dev-master",
        "granam/exception-hierarchy": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "0.9.*"
    }
}
